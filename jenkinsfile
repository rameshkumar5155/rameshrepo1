
pipeline  {
    agent any
    stages {
        stage('build') {
            steps  {
            echo "building the project ........."
            bat "mvn clean"
            }
}
        stage('test') {
            steps {
            echo "deploy the project ........."
            bat "mvn test"
  }
}
        stage('compile') {
            steps {
            echo "testing  the project ........."
            bat "mvn compile"
           
  }
}
        stage('release') {
            steps  {
            echo "release  the project ........."
      }
   }
  }
}
